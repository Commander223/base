<!--+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
<!--                                                                     -->
<!--                       UNICODE ENTITIES                              -->
<!--        new format is "U+XXXX"  with the high byte appearing 1st     -->
<!--                                                                     -->
<!--+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->

<!-- THESE ENTITIES SHOULD HAVE THE CORRECT UNICODE VALUES               -->
<!--                                                                     -->
<!--                                                                     -->

<!ENTITY half   SDATA "U+00BD"--=fraction one-half-->
<!ENTITY frac12 SDATA "U+00BD"--=fraction one-half-->
<!ENTITY frac14 SDATA "U+00BC"--=fraction one-quarter-->
<!ENTITY frac34 SDATA "U+00BE"--=fraction three-quarters-->
<!ENTITY sup1   SDATA "U+00B9"--=superscript one-->
<!ENTITY sup2   SDATA "U+00B2"--=superscript two-->
<!ENTITY sup3   SDATA "U+00B3"--=superscript three-->

<!ENTITY plus   SDATA "U+002B"--=plus sign B:-- >
<!ENTITY plusmn SDATA "U+00B1"--/pm B: =plus-or-minus sign-->
<!ENTITY lt     SDATA "U+003C"--=less-than sign R:-->
<!ENTITY equals SDATA "U+003D"--=equals sign R:-->
<!ENTITY gt     SDATA "U+003E"--=greater-than sign R:-->
<!ENTITY divide SDATA "U+00F7"--/div B: =divide sign-->
<!ENTITY times  SDATA "U+00D7"--/times B: =multiply sign-->

<!ENTITY curren SDATA "U+00A4"--=general currency sign-->
<!ENTITY pound  SDATA "U+00A3"--=pound sign-->
<!ENTITY dollar SDATA "U+0024"--=dollar sign-->
<!ENTITY cent   SDATA "U+00A2"--=cent sign-->
<!ENTITY yen    SDATA "U+00A5"--/yen =yen sign-->

<!ENTITY num    SDATA "U+0023"--=number sign-->
<!ENTITY percnt SDATA "U+0025"--=percent sign-->
<!ENTITY amp    SDATA "U+0026"--=ampersand-->
<!ENTITY ast    SDATA "U+002A"--/ast B: =asterisk-->
<!ENTITY commat SDATA "U+0040"--=commercial at-->
<!ENTITY lsqb   SDATA "U+005B"--/lbrack O: =left square bracket-->
<!ENTITY bsol   SDATA "U+005C"--/backslash =reverse solidus-->
<!ENTITY rsqb   SDATA "U+005D"--/rbrack C: =right square bracket-->
<!ENTITY lcub   SDATA "U+007B"--/lbrace O: =left curly bracket-->
<!ENTITY horbar SDATA "[horbar]"--=horizontal bar-->
<!ENTITY verbar SDATA "U+007C"--/vert =vertical bar-->
<!ENTITY rcub   SDATA "U+007D"--/rbrace C: =right curly bracket-->
<!ENTITY micro  SDATA "U+00B5"--=micro sign-->
<!ENTITY deg    SDATA "U+00B0"--=degree sign-->
<!ENTITY sect   SDATA "U+00A7"--=section sign-->
<!ENTITY para   SDATA "U+00B6"--=pilcrow (paragraph sign)-->
<!ENTITY middot SDATA "U+00B7"--/centerdot B: =middle dot-->
<!ENTITY copy   SDATA "U+00A9"--=copyright sign-->
<!ENTITY reg    SDATA "U+00AE"--/circledR =registered sign-->
<!ENTITY brvbar SDATA "U+00A6"--=broken (vertical) bar-->
<!ENTITY not    SDATA "U+00AC"--/neg /lnot =not sign-->

<!ENTITY excl   SDATA "U+0021"--=exclamation mark-->
<!ENTITY iexcl  SDATA "U+00A1"--=inverted exclamation mark-->
<!ENTITY quot   SDATA "U+0022"--=(double quote) quotation mark-->
<!ENTITY apos   SDATA "U+0027"--=apostrophe-->
<!ENTITY lpar   SDATA "U+0028"--O: =left parenthesis-->
<!ENTITY rpar   SDATA "U+0029"--C: =right parenthesis-->
<!ENTITY comma  SDATA "U+002C"--P: =comma-->
<!ENTITY lowbar SDATA "U+005F"--=low line-->
<!ENTITY hyphen SDATA "U+002D"--=hyphen-->
<!ENTITY period SDATA "U+002E"--=full stop, period-->
<!ENTITY sol    SDATA "U+002F"--=solidus-->
<!ENTITY colon  SDATA "U+003A"--/colon P:-->
<!ENTITY semi   SDATA "U+003B"--=semicolon P:-->
<!ENTITY quest  SDATA "U+003F"--=question mark-->
<!ENTITY iquest SDATA "U+00BF"--=inverted question mark-->
<!ENTITY laquo  SDATA "U+00AB"--=angle quotation mark, left-->
<!ENTITY raquo  SDATA "U+00BB"--=angle quotation mark, right-->
<!ENTITY lsquo  SDATA "U+2018"--=single quotation mark, left-->
<!ENTITY rsquo  SDATA "U+2019"--=single quotation mark, right-->
<!ENTITY ldquo  SDATA "U+201C"--=double quotation mark, left-->
<!ENTITY rdquo  SDATA "U+201D"--=double quotation mark, right-->
<!ENTITY nbsp   SDATA "U+0020"--=no break (required) space-->
<!ENTITY shy    SDATA "U+00AD"--=soft hyphen-->


<!--93/07/21                                                             -->
<!--UNICODE VALUES THAT DON'T MAP TO ANSI CODES ARE CODED AS             -->
<!--"[entity-name]" AND DISPLAYED AS "&entity-name;"                     -->

<!ENTITY trade  SDATA "U+2122"--=trade mark sign-->

<!ENTITY larr   SDATA "[larr]"--/leftarrow /gets A: =leftward arrow-->
<!ENTITY rarr   SDATA "[rarr]"--/rightarrow /to A: =rightward arrow-->
<!ENTITY uarr   SDATA "[uarr]"--/uparrow A: =upward arrow-->
<!ENTITY darr   SDATA "[darr]"--/downarrow A: =downward arrow-->

<!--                                                                     -->
<!--                                                                     -->
<!-- END OF CORRECT UNICODE VALUES                                       -->
 
<!--93/07/21                                                             -->
<!--UNICODE VALUES THAT DON'T MAP TO ANSI CODES ARE CODED AS             -->
<!--"[entity-name]" AND DISPLAYED AS "&entity-name;"                     -->

<!ENTITY frac18 SDATA "[frac18]"--=fraction one-eighth-->
<!ENTITY frac38 SDATA "[frac38]"--=fraction three-eighths-->
<!ENTITY frac58 SDATA "[frac58]"--=fraction five-eighths-->
<!ENTITY frac78 SDATA "[frac78]"--=fraction seven-eighths-->
<!ENTITY sung   SDATA "[sung]"--=music note (sung text sign)-->
<!ENTITY ordm   SDATA "[ordm]"--=ordinal indicator, masculine-->
<!ENTITY ordf   SDATA "[ordf]"--=ordinal indicator, feminine-->
<!ENTITY ohm    SDATA "[ohm]"--=ohm sign-->
