<!-- ****************** GROUPED ATTRIBUTES *************************         -->
<!--                                                                         -->

<!-- Yes or No as numeric 1=Yes 0=No                                                        -->
<!ENTITY % yorn "NAME" >

<!ENTITY % mediafmt " TIF | CGM | JPG | PDF ">
<!--                                                                         -->
<!-- Types of embedded media and captions                                   -->
<!ENTITY % img  "emi | emr | ele "                          
>
<!-- Types of highlighting, superscripts, subscripts, references, and floating accents   -->
<!ENTITY % hil  "b | i | o | u | bai | han | sp | sb | chf | fla | clrf |chrf"          >

<!-- Types of lists                                                          -->
<!ENTITY % lst  "dl | ol | sl | ul"            >

<!-- Types of math  markup                                                       -->
<!ENTITY % math " |math | dfref | f  | df | dfg " >

<!ENTITY % table " | table | tblref " >

<!-- Five types of complex material inside or outside of paragraphs          -->
<!-- bioseq is forseen for later use                                         -->
<!ENTITY % compl "(%lst;) %table; %math; | chem | bioseq | artifact | pageimg"   >

<!-- Contents of a paragraph  with Complex Work Units                             -->
<!ENTITY % ptext "#PCDATA | (%hil;) | (%img;) | (%compl;) "   >






<!-- ****************** GROUPED ENTITIES ***************************         -->

<!-- Name and identification of an organization                              -->
<!ENTITY % ORG   "ONM,(ODV,DID?)*"                                       
>
<!-- Name of an individual                                                   -->
<!ENTITY % NAM   "TTL?,FNM,SNM,SFX?"                          
>
<!-- Components of an address                                                -->
<!ENTITY % ADR "PBX?,STR*,CITY,STA,CTRY,PCODE,EAD*,
                 TEL*,FAX*"                                                   
>
<!-- Components of party                                                     -->
<!ENTITY % PARTY "((%NAM;)|(%ORG;)),(%ADR;),DTXT?,RCTRY?,NCTRY?"             
>


<!--                     Math and Table Inclusions                                     
      Future versions of this DTD may include provisions for the use of ISO12083 math markup -->

<!ENTITY % mathpac PUBLIC 
                     "-//USA-DOD//DTD SUP MIL-M-28001 MATHPACK 911001//EN"> 
              %mathpac;    
    

<!-- The following elements/entities are redefined to accomodate the CALS tables-->


<!ENTITY % paracon "( #PCDATA | (%hil;) | (%img;) | (%compl;))" >
<!ENTITY % titles "( #PCDATA | (%hil;) | (%img;) | (%compl;))" >
<!ELEMENT para  - - ((#PCDATA | (%hil;) | (%img;) | (%compl;))+)    >
<!ELEMENT tblref  - o    EMPTY     -- Table reference -->
<!ATTLIST   tblref     id  IDREFS     #REQUIRED            -- Table reference(s)     -->


<!ENTITY % tablepac PUBLIC 
                   "-//USA-DOD//DTD CALS MIL-M-28001 TABLEPAK 950127 //EN"> 
              %tablepac;

<!-- ********** Amendment Request Model  *********************         -->
<!ENTITY % PTOamd PUBLIC  "-//USPTO//DTD Amendment Request 970331//EN">
%PTOamd;
<!--    ******** COMMON DOCUMENT ELEMENTS *********       -->

<!-- ******* MAJOR CONTENTS OF SUBDOCUMENTS ***********   -->
<!--                                                                         -->
<!ELEMENT h     - - (%ptext;)                 -- Header                      -->
<!ATTLIST h    lvl NUMBER #IMPLIED            -- Header level                --
                         id      ID     #REQUIRED            -- Unique  ID        -->

<!ELEMENT t     - - (#PCDATA)              -- Paragraph elements          -->


<!ELEMENT p     - - ((%ptext;)+)              -- Paragraph elements          -->
<!ATTLIST p    id   ID      #REQUIRED            -- Reference number            -->

<!-- ************ HIGHLIGHTED ELEMENTS **************            -->
<!--                                                                         -->
<!ELEMENT b     - - (%ptext;)+ -(b|fla)       -- Bold typeface               -->
<!ELEMENT i     - - (%ptext;)+ -(i)           -- Italics                     -->
<!ELEMENT bai   - - (%ptext;)+ -(bai|han)     -- Expanded font               -->
<!ELEMENT han   - - (%ptext;)+ -(han|bai)     -- Compressed font             -->
<!ELEMENT o     - - (%ptext;)+ -(o)           -- Overscore                   -->
<!ELEMENT u     - - (%ptext;)+ -(u)           -- Underscore                  -->
<!ATTLIST (o|u) st  (S|D|T|B|DA|DT) "S"       -- Line style (default single) --
              type  (dot|dotdot|dot3|dot4|tie|tiebrace|
                     hat|haczeck|acute|grave|cedil|ring|macron|
                     ogonek|dblac|breve|tilde|vec|rvec|dyad|bar)
                       "bar" -- types of over and under scores (default bar) -->

<!-- ************** SUPER/SUBSCRIPTS **********************             -->
<!--                                                                         -->
<!ELEMENT sp    - - (%ptext;)* -(fla)         -- Superscript                 -->
<!ELEMENT sb    - - (%ptext;)* -(fla)         -- Subscript                   -->
<!ATTLIST (sp|sb) pos (PRE|MID|POST) "POST"   -- Position (default post)     -->
<!--                                                                         -->
<!-- ************** CONSTRUCTS *****************************             -->
<!--                                                                         -->
<!ELEMENT chf   - - (t,(bar|commat),t)  -- Character fraction    -->
<!ELEMENT bar    - o  EMPTY                   -- Causes horizontal line      -->
<!ELEMENT commat - o  EMPTY                   -- No horizontal separator     -->
<!ELEMENT fla   - - (t,bar,t)     -- Floating accent (overstrike)-->
<!--                                                                         -->

<!-- ********** ALL KINDS OF LISTS ****************************              -->
<!--                                                                         -->
<!ELEMENT dl   - - (dt,dd)+                   -- Definition list             -->
<!ATTLIST dl   tsize NUMBER #IMPLIED          -- Term size attribute         --
               		compact (compact) #IMPLIED     -- Spacing between items       --
		  id      ID     #REQUIRED            -- Unique  ID        -->
<!ELEMENT dt   - o (%ptext;)                  -- Definition term             -->
<!ELEMENT dd   - o ((%ptext;)|p)+             -- Definition description      -->
<!ELEMENT ol   - - ((li+)|(claim+))             -- Ordered list                -->
<!ATTLIST ol   compact (compact) #IMPLIED     -- Spacing between items       --
               level  NUMBER #IMPLIED         -- Nesting level of list       --
               prefix CDATA #IMPLIED          -- Prefix for each list item   --
               numstyle CDATA #IMPLIED        -- Numbering style             --
                id      ID     #REQUIRED            -- Unique  ID        -->
<!ELEMENT sl    - - (li)+                     -- Simple list                 -->
<!ATTLIST sl  compact (compact) #IMPLIED      -- Spacing between items       --
              level  NUMBER #IMPLIED          -- Nesting level of list       --
	  id      ID     #REQUIRED            -- Unique  ID        -->
<!ELEMENT ul    - -  (li)+                    -- Unordered list              -->
<!ATTLIST ul  st  CDATA  #REQUIRED            -- Ulist symbol                --
              level  NUMBER #IMPLIED          -- Nesting level of list       --
              compact (compact) #IMPLIED      -- Spacing between items       --
	     id      ID     #REQUIRED            -- Unique  ID        -->
<!ELEMENT li    - o ((%ptext;)|p)+            -- List item                   -->
<!ELEMENT claim   - - ((%ptext;)|p)+  -- Text of claim           -->
<!ATTLIST claim    id   ID #REQUIRED                          -- Unique claim ID -- >

<!--                                                                         -->
<!-- ***********MEDIA RELATED ELEMENTS*************************              -->
<!--    -->
<!ELEMENT emi   - o  EMPTY                    -- Embedded media              -->
<!ATTLIST emi  id  ID #REQUIRED         -- Unique identity              --
               he  NUMBER  #IMPLIED          -- Height in mm                --
               wi  NUMBER  #IMPLIED          -- Width in mm                 --
               lx  NUMBER  #IMPLIED           -- X-coord 1/10 mm             --
               ly  NUMBER  #IMPLIED           -- Y-coord 1/10 mm             --
               boardno ENTITY #REQUIRED  -- external entity reference --
               imf (%mediafmt;)  #REQUIRED       -- Format stored emi           -->

<!ELEMENT emr   - o EMPTY                     -- Reference to emi or page image           -->
<!ATTLIST emr  id  IDREFS  #REQUIRED       >

<!ELEMENT ele   - - (%ptext;)+                -- Media object caption              -->
<!ATTLIST ele  id  ID  #REQUIRED           >

<!-- *************** Artifacts  and Page Images*****************   -->
<!--                                                                         -->
<!ELEMENT pageimg   - o EMPTY    -- A page image description             -->
<!ATTLIST pageimg  id  ID #IMPLIED         -- Image identity              --
   	  boardno ENTITY #REQUIRED
          desc    CDATA #IMPLIED  -- Description --
  	  pimf (TIF | PDF)  #REQUIRED    -- Format stored page image        -->


<!--  Artifacts are be pointers to external items such as models, 
        video tapes, faxes, etc. Binary types may be pointers to refer to
        electronically storable items such as computer source code or other
        generic binary data    -->

<!ELEMENT artifact   - o EMPTY      -- Artifact Reference        -->
<!ATTLIST   artifact
              id   ID  #IMPLIED                        --  Unique identifier        --
              afmt (BIN|OTHER) "OTHER"             --  artifact format        --
              boardno ENTITY     #IMPLIED        -- File Entity if binary                  --
              desc  CDATA #IMPLIED              -- Description --
              artfmt CDATA #IMPLIED             --  binary format        -->
<!-- ********** CHEMISTRY NOTATION  *********              -->
<!-- Future versions of this DTD may include provisions for the use of 
        Chemical Markup Language (CML)     -->
<!--                                                                         -->
<!ELEMENT (chem)   - o  EMPTY     -- Chemical Notation              -->
<!ATTLIST   chem 
              id   ID  #IMPLIED                      -- Chemical expression id        --
              he  NUMBER  #IMPLIED          -- Height in mm                --
              wi  NUMBER  #IMPLIED          -- Width in mm                 --
              lx  NUMBER  #IMPLIED           -- X-coord 1/10 mm             --
              ly  NUMBER  #IMPLIED           -- Y-coord 1/10 mm             --
              cfmt ( MOL | CHD | CGM | TIF)   #REQUIRED -- Chem File format      --
              boardno ENTITY     #REQUIRED        -- File Entity                    -->
<!ELEMENT chrf   - o EMPTY                     -- Reference to chemical expression           -->
<!ATTLIST chrf  id  IDREFS  #REQUIRED       >

<!--                                                                         -->
<!-- ****** MATH NOTATION  *********              -->

<!--                                                                         -->
<!ELEMENT (math)   - o EMPTY    -- Math Expression            -->
<!ATTLIST math  num CDATA #IMPLIED               
                     id  ID  #IMPLIED           -- Math expression id   --
                     mfmt (TEX | CGM | TIF )   #REQUIRED  -- Math format --
                     boardno ENTITY     #REQUIRED        -- File Entity                    -->


<!--                                                                         -->
<!-- *************** Miscellaneous *********************************         -->
<!--                                                                         -->
<!ELEMENT br    - o EMPTY                     -- Line break                  -->
<!ELEMENT foo   - - (%ptext;)+                -- Footnotes                   -->
<!ATTLIST foo  fn  NUTOKEN  #REQUIRED         -- Footnote id.                -->
<!ELEMENT for   - - (%ptext;)+                -- Footnote reference          -->
<!ATTLIST for  id  IDREFS  #REQUIRED         -- Footref id.                 -->
<!ELEMENT clrf  - o EMPTY                -- Reference to a claim        -->
<!ATTLIST    clrf     id  IDREFS     #REQUIRED            -- Unique  ID        -->

<!--                                                                         -->
<!-- ********** BIOLOGICAL SEQUENCES *******************************         -->
<!-- This  section forseen for later use (PATENTIN)                          -->
<!--                                                                         -->
<!ELEMENT bioseq - - CDATA                    -- (Stub for later expansion)  -->
<!ATTLIST bioseq   id ID #REQUIRED          -- Reference number            -->


<!ELEMENT 	fni 	- - 	((%PARTY;), petn, authrty)   -- First Named Inventor -->
<!ATTLIST    	fni id     ID 	#REQUIRED	-- Unique  ID        -->   
<!ELEMENT 	docket 	- - 	(#PCDATA)  	-- Attorney Docket Number  	-->
<!ELEMENT 	petn 	- - 	(#PCDATA)  	-- Unsigned Petition Filed? Y/N 	-->
<!ELEMENT 	authrty - - 	(#PCDATA)  	-- Inventor Authority 	-->

<!-- Components of a name (NAM)                                              -->
<!ELEMENT TTL  - o (#PCDATA)       -- Title (e.g., Mr., Mrs., Dr., CPT)      -->
<!ELEMENT FNM  - o (#PCDATA)       -- Given and middle name(s) or initials   -->
<!ELEMENT SNM  - o (#PCDATA)       -- Family, last, or surname               -->
<!ELEMENT SFX  - o (#PCDATA)       -- Suffix (e.g., II, Jr., Esq. et al.)    -->
<!--                                                                         -->
<!-- Components of an organization (ORG)                                     -->
<!ELEMENT ONM  - o (#PCDATA)       -- Organization name                      -->
<!ELEMENT ODV  - o (#PCDATA)       -- Division of organization               -->
<!ELEMENT DID  - o (#PCDATA)       -- Identifying number of division         -->
<!--                                                                         -->
<!--  Components of an address (ADR)                                         -->
<!ELEMENT PBX  - o (#PCDATA)       -- Post Office box number                 -->
<!ELEMENT STR  - o (#PCDATA)       -- Street, house number or name,          -->
<!ELEMENT CITY  - o (#PCDATA)      -- City or town                           -->
<!ELEMENT STA  - o (#PCDATA)       -- Region of country (state, province)    -->
<!ELEMENT CTRY - o (#PCDATA)       -- Country (ST.3)                         -->
<!ELEMENT PCODE - o (#PCDATA)      -- Postal Code or zip code                -->
<!ELEMENT EAD  - o (#PCDATA)       -- Electronic address (e.g., email)       -->
<!ELEMENT TEL  - o (#PCDATA)       -- Telephone number w/ regional/area code -->
<!ELEMENT FAX  - o (#PCDATA)       -- Facsimile telephone number             -->
<!--                                                                         -->
<!--  Components of party (PARTY)                                            -->
<!--      NAM                         Name of person                         -->
<!--      ORG                         Name of organization                   -->
<!--      ADR                         Address                                -->
<!ELEMENT DTXT  - o (#PCDATA)      -- Descriptive text                       -->
<!ELEMENT RCTRY - o (CTRY)         -- Country of residence                   -->
<!ELEMENT NCTRY - o (CTRY)         -- Country of nationality                 -->
<!--  Components of a date(DATE)                                             -->
<!ELEMENT DATE - o (#PCDATA,TIME?) -- YYYYMMDD                               -->
<!ELEMENT TIME - o (#PCDATA)       -- HHMMSS (UCT)                           -->
<!ELEMENT appnr - - (#PCDATA)  -- application nr  -- >
<!ELEMENT filedate - - (DATE)  -- Filing date  -- >



