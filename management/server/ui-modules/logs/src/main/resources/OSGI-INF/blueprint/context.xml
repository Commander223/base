<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">
    
    <reference id="someApi" availability="mandatory" interface="org.safehaus.kiskis.mgmt.api.SomeApi"/>    
    <reference id="dbManager" availability="mandatory" interface="org.safehaus.kiskis.mgmt.api.dbmanager.DbManager"/>

    <service ref="module" interface="org.safehaus.kiskis.mgmt.server.ui.services.Module"/>
    <bean id="module" class="org.safehaus.kiskis.mgmt.server.ui.modules.logs.LogsModule">
        <property name="someApi" ref="someApi"/>                   
        <property name="dbManager" ref="dbManager"/>                   
    </bean>  
      
</blueprint>
